    a lv2:ControlPort, lv2:InputPort;
    lv2:index 0;
    lv2:default 0;
    lv2:maximum 1;
    lv2:minimum 0;
    lv2:name "A: Active";
    lv2:symbol "a_act";
    lv2:portProperty lv2:integer, lv2:toggled;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 1;
    lv2:default 3500.0;
    lv2:maximum 14000.0;
    lv2:minimum 20.0;
    lv2:name "A: Frequency";
    lv2:symbol "a_f";
    lv2:portProperty epp:logarithmic;
    units:unit units:hz;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
