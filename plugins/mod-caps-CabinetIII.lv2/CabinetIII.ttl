    a lv2:ControlPort, lv2:InputPort;
    lv2:index 0;
    lv2:default 1;
    lv2:minimum 0;
    lv2:maximum 16;
    lv2:name "Model";
    lv2:symbol "model";
    lv2:portProperty lv2:enumeration, lv2:integer;
    lv2:scalePoint
