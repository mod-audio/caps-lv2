[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 1;
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
    lv2:name "Alt";
    lv2:symbol "alt";
    lv2:portProperty lv2:integer, lv2:toggled;
],
[
