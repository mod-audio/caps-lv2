@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix mod:  <http://moddevices.com/ns/mod#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

<http://moddevices.com/plugins/caps/PlateX2>
a lv2:ReverbPlugin, lv2:Plugin;
lv2:microVersion 24;
lv2:minorVersion 9;
lv2:optionalFeature lv2:hardRTCapable;
mod:brand "CAPS";
mod:label "PlateX2";

rdfs:comment """
This version of the Plate reverberator comes with stereo inputs.

source: http://quitte.de/dsp/caps.html#Plate
""";

lv2:port
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 0;
    lv2:default 0.9995;
    lv2:maximum 1.0;
    lv2:minimum 0.0;
    lv2:name "Bandwidth";
    lv2:symbol "bandwidth";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 1;
    lv2:default 0.5;
    lv2:maximum 1.0;
    lv2:minimum 0.0;
    lv2:name "Tail";
    lv2:symbol "tail";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:default 0.0005;
    lv2:maximum 1.0;
    lv2:minimum 0.0;
    lv2:name "Damping";
    lv2:symbol "damping";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:default 0.25;
    lv2:maximum 1.0;
    lv2:minimum 0.0;
    lv2:name "Blend";
    lv2:symbol "blend";
],
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 4;
    lv2:name "In Left";
    lv2:symbol "inl";
],
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 5;
    lv2:name "In Right";
    lv2:symbol "inr";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 6;
    lv2:name "Out Left";
    lv2:symbol "outl";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 7;
    lv2:name "Out Right";
    lv2:symbol "outr";
];

doap:license <http://usefulinc.com/doap/licenses/gpl>;

doap:developer [
    foaf:homepage <http://quitte.de/dsp/caps.html>;
    foaf:mbox <mailto:tim@quitte.de>;
    foaf:name "Tim Goetze";
];

doap:maintainer [
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:devel@moddevices.com>;
    foaf:name "MOD Team";
];

doap:name "C* PlateX2 - Stereo in/out Versatile plate reverb".
