@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix mod:  <http://moddevices.com/ns/mod#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

<http://moddevices.com/plugins/caps/Eq10>
a lv2:EQPlugin, lv2:Plugin;
lv2:microVersion 26;
lv2:minorVersion 9;
lv2:optionalFeature lv2:hardRTCapable;
mod:brand "CAPS";
mod:label "Eq10";

rdfs:comment """
A classic octave-band biquad-filter design, basically a direct digital translation of the analog original. There's also a stereo version (Eq10X2).

Frequency bands centered above Nyquist are automatically disabled.

source: http://quitte.de/dsp/caps.html#Eq10
""";

lv2:port
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 0;
    lv2:default 0.0;
    lv2:maximum 24.0;
    lv2:minimum -48.0;
    lv2:name "31 Hz";
    lv2:symbol "band31hz";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 1;
    lv2:default 0.0;
    lv2:maximum 24.0;
    lv2:minimum -48.0;
    lv2:name "63 Hz";
    lv2:symbol "band63hz";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:default 0.0;
    lv2:maximum 24.0;
    lv2:minimum -48.0;
    lv2:name "125 Hz";
    lv2:symbol "band125hz";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:default 0.0;
    lv2:maximum 24.0;
    lv2:minimum -48.0;
    lv2:name "250 Hz";
    lv2:symbol "band250hz";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 4;
    lv2:default 0.0;
    lv2:maximum 24.0;
    lv2:minimum -48.0;
    lv2:name "500 Hz";
    lv2:symbol "band500hz";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 5;
    lv2:default 0.0;
    lv2:maximum 24.0;
    lv2:minimum -48.0;
    lv2:name "1 kHz";
    lv2:symbol "band1khz";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 6;
    lv2:default 0.0;
    lv2:maximum 24.0;
    lv2:minimum -48.0;
    lv2:name "2 kHz";
    lv2:symbol "band2khz";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 7;
    lv2:default 0.0;
    lv2:maximum 24.0;
    lv2:minimum -48.0;
    lv2:name "4 kHz";
    lv2:symbol "band4khz";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 8;
    lv2:default 0.0;
    lv2:maximum 24.0;
    lv2:minimum -48.0;
    lv2:name "8 kHz";
    lv2:symbol "band8khz";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 9;
    lv2:default 0.0;
    lv2:maximum 24.0;
    lv2:minimum -48.0;
    lv2:name "16 kHz";
    lv2:symbol "band16khz";
],
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 10;
    lv2:name "In";
    lv2:symbol "in";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 11;
    lv2:name "Out";
    lv2:symbol "out";
];

